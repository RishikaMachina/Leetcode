#Runtime: 6188 ms
#Memory Usage: 13.6 MB

#memory usage beats 95.43% of python3 submissions

class Solution():
    def twoSum(self, nums: List[int], target: int) -> List[int]:
        a=[]
        for i in range(0,len(nums)-1):
            for j in range(i+1,len(nums)):
                if(nums[j] == target - nums[i]):
                    a.append(i)
                    a.append(j)
                else:
                    continue
        return a
